{
  "name": "pegasys-orchestrate",
  "version": "1.0.0-rc1",
  "description": "The PegaSys Orchestrate library provides convenient access to the PegaSys Orchestrate API from applications written in server-side JavaScript",
  "scripts": {
    "clean": "rimraf ./coverage ./build ./lib",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint src",
    "test:clean": "cross-env NODE_ENV=test jest --clearCache",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cover": "cross-env NODE_ENV=test jest --coverage",
    "coveralls": "npm run test:cover && cat ./coverage/lcov.info | coveralls",
    "build": "webpack --mode production",
    "contract-registry": "contract-registry"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "kafka",
    "pegasys",
    "protobuf",
    "orchestrate",
    "transaction"
  ],
  "files": [
    "build",
    "typedefinitions",
    "types"
  ],
  "author": "ConsenSys PegaSys Engineering",
  "contributors": [
    "Alexandre Belling <alexandre.belling@consensys.net>",
    "Alexis Masseron <alexis.masseron@consensys.net>",
    "Nicolas Maurice <nicolas.maurice@consensys.net>",
    "Christian Tran <christian.tran@consensys.net>"
  ],
  "license": "Apache-2.0",
  "main": "./build/library.bundled.js",
  "types": "./typedefinitions/index.d.ts",
  "bin": {
    "contract-registry": "./build/contractregistry.bundled.js",
    "wallet": "./build/wallet.bundled.js"
  },
  "devDependencies": {
    "@babel/cli": "~7.2.3",
    "@babel/core": "~7.3.4",
    "@babel/plugin-proposal-class-properties": "~7.3.4",
    "@babel/plugin-proposal-json-strings": "~7.2.0",
    "@babel/plugin-syntax-dynamic-import": "~7.2.0",
    "@babel/plugin-syntax-import-meta": "~7.2.0",
    "@babel/plugin-transform-runtime": "~7.3.4",
    "@babel/preset-env": "~7.3.4",
    "babel-eslint": "~10.0.1",
    "babel-jest": "~24.8.0",
    "babel-loader": "~8.0.5",
    "coveralls": "~3.0.3",
    "cross-env": "~5.2.0",
    "enzyme": "~3.9.0",
    "eslint": "~5.15.1",
    "eslint-loader": "~2.1.2",
    "eslint-plugin-jest": "~22.3.2",
    "jest": "~24.1.0",
    "rimraf": "~2.6.3",
    "typescript": "~3.5.2",
    "webpack": "~4.29.6",
    "webpack-cli": "~3.2.3",
    "webpack-node-externals": "~1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "~7.3.4",
    "@babel/register": "~7.0.0",
    "@grpc/proto-loader": "^0.5.1",
    "@types/web3": "~1.0.19",
    "arg": "^4.1.1",
    "bimap": "0.0.15",
    "commander": "^3.0.2",
    "core-js": "~3.1.4",
    "global": "^4.4.0",
    "google-protobuf": "~3.8.0",
    "grpc": "~1.24.2",
    "kafka-node": "~4.1.3",
    "node-gyp": "^6.0.0",
    "path": "^0.12.7",
    "regenerator-runtime": "~0.13.3",
    "shebang-loader": "^0.0.1",
    "uuid": "~3.3.2",
    "web3": "~1.2.0",
    "web3-core": "~1.2.0",
    "web3-core-requestmanager": "~1.2.0",
    "web3-utils": "~1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/broker/types/*/*_pb.js"
    ]
  }
}
